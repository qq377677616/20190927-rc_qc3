/**index.wxss**/

page {
  background-color: #fff;
}

.index_page {
  padding-bottom: 100rpx;
}

/* swiper_area */

.swiper_area {
  height: 519rpx;
  position: relative;
}

.swiper_area swiper {
  height: 100%;
}

.swiper_area swiper swiper-item {
  background-size: cover;
  background-repeat: no-repeat;
  background-position: center center;
}

.swiper_area swiper swiper-item button {
  width: 100%;
  height: 100%;
}

.swiper_area .dots {
  display: flex;
  position: absolute;
  bottom: 43rpx;
  left: 50%;
  z-index: 1;
  transform: translatex(-50%);
}

.swiper_area .dots .dot_item {
  width: 8rpx;
  height: 8rpx;
  border-radius: 50%;
  background-color: #fff;
  margin-left: 15rpx;
  margin-right: 5rpx;
  position: relative;
}

.swiper_area .dots .dot_item .tiao {
  position: absolute;
  left: 0;
  top: 0;
  height: 8rpx;
  width: 0;
  opacity: 0;
  border-radius: 8rpx;
  background-color: #fff;
  /* transition:width 500ms ease-in-out,opacity 500ms ease-in-out; */
}

.swiper_area .dots .dot_item.active .tiao {
  /* width: 26rpx; *//* border: 1rpx solid red; */
  opacity: 1;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  background-color: transparent;
  width: 18rpx;
  height: 18rpx;
  border-radius: 50%;
  border: 1rpx solid white;
  /* background-color: red; */
}

/* swiper_area */

.main_container {
  /* transform:translatey(-30rpx); */
  background-color: #fff;
  overflow: hidden;
  /* border-radius: 30rpx; */
}

.main_container scroll-view {
}

/* nav_area */

.main_container .nav_area {
  padding: 31rpx 0 58rpx;
  display: flex;
  flex-wrap: nowrap;
}

.main_container .nav_area .nav_item {
  width: 25%;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.main_container .nav_area .nav_item.flex1 {
  flex: 1;
}

.main_container .nav_area .nav_item.flex0 {
  flex: 0 0 auto;
}

.main_container .nav_area .nav_item .image {
  width: 80rpx;
  height: 80rpx;
  margin-bottom: 20rpx;
}

.main_container .nav_area .nav_item .image image {
  width: 100%;
  height: 100%;
}

.main_container .nav_area .nav_item .span {
  width: 100%;
  text-align: center;
  color: #333;
  font-size: 26rpx;
  font-family: PingFang SC;
}

/* nav_area */

/* list_area */

.list_area {
  padding: 0 29rpx;
}
.list_area .tab-bar{
  display: flex;
}
.list_area .tab-bar scroll-view{
  flex:1;
  margin-right: 10rpx;
}
.list_area .tab-bar .position {
  flex:0 0 auto;
  display: flex;
  align-items: center;
  max-width: 170rpx;
}

.list_area .tab-bar .position .cur-position-icon {
  flex-grow: 0;
  flex-shrink: 0;
  width: 26rpx;
  margin-right: 8rpx;
}

.list_area .tab-bar .position .cur-position-text {
  color: #000;
  font-size: 26rpx;
}
.list_area scroll-view .title {
  display: flex;
}

.list_area scroll-view .title .title_item {
  position: relative;
  flex-basis: auto;
  flex-shrink: 0;
  flex-grow: 0;
  margin-right: 28rpx;
  
}

.list_area scroll-view .title .title_item .span {
  padding-bottom: 10rpx;
  font-size: 28rpx;
  color: #333;
  line-height: 44rpx;
}

.list_area scroll-view .title .title_item .span.active {
  font-size: 34rpx;
  font-weight: bold;
}

.list_area scroll-view .title .title_item .border {
  width: 40rpx;
  height: 4rpx;
  background-color: #333;
  border-radius: 2rpx;
}

.list_area .list {
  padding: 27rpx 0 0;
}

.list_area .list .list_item {
  background-size: cover;
  background-repeat: no-repeat;
  /* height: 300rpx; */
  position: relative;
}

.list_area .list .list_item:not(:first-child) {
  margin-top: 30rpx;
}

.list_area .list .list_item:not(:last-child) {
  margin-bottom: 40rpx;
}

.list_area .list .list_item .describe {
  position: absolute;
  top: 55rpx;
  height: 0;
  border-style: solid;
  width: 370rpx;
  box-sizing: border-box;
}

.list_area .list .list_item.right .describe {
  right: 0;
  border-color: rgba(0, 0, 0, 0.6) transparent transparent transparent;
  border-width: 60rpx 0 0 60rpx;
}

.list_area .list .list_item.left .describe {
  left: 0;
  border-color: transparent transparent rgba(0, 0, 0, 0.6) transparent;
  border-width: 0 60rpx 60rpx 0;
}

.list_area .list .list_item .describe .main {
  position: absolute;
  width: 370rpx;
  height: 60rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  box-sizing: border-box;
}

.list_area .list .list_item.right .describe .main {
  left: -60rpx;
  top: -60rpx;
  padding-left: 60rpx;
}

.list_area .list .list_item.left .describe .main {
  right: -60rpx;
  top: 0;
  padding-right: 60rpx;
}

.list_area .list .list_item .describe .main .span {
  font-size: 30rpx;
  color: rgba(255, 255, 255, 1);
  line-height: 1em;
  text-align: center;
}

.list_area .list .list_item .describe .main .time {
  font-size: 18rpx;
  font-weight: bold;
  color: rgba(255, 255, 255, 1);
  line-height: 1em;
  text-align: center;
}

/* list_area */

/* tips_mask */

.tips_mask {
  width: 100vw;
  height: 100vh;
  position: fixed;
  left: 50%;
  top: 50%;
  margin-left: -50vw;
  margin-top: -50vh;
  z-index: 999;
  background-size: cover;
  background-repeat: no-repeat;
  background-position: center center;
  background-color: #fff;
}

.tips_mask .main {
  background: rgba(8, 8, 8, 0.4);
  border-radius: 16rpx;
  width: 660rpx;
  padding: 38rpx 73rpx 45rpx;
  box-sizing: border-box;
  position: absolute;
  bottom: 213rpx;
  left: 50%;
  transform: translatex(-50%);
  letter-spacing: 0;
}

.tips_mask .main .title {
  font-size: 36rpx;
  color: #f8f8f8;
  text-align: center;
}

.tips_mask .main .number {
  font-size: 72rpx;
  /* color: #FDE24A; */
  color: #ecb1ac;
  text-align: center;
  line-height: 100rpx;
  margin-bottom: 37rpx;
}

.tips_mask .main .btn_group {
  display: flex;
  justify-content: space-between;
}

.tips_mask .main .btn_group .btn {
  box-sizing: border-box;
  min-width: 219rpx;
  height: 57rpx;
  border-radius: 30rpx;
  font-size: 30rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.tips_mask .main .btn_group .btn.out {
  background-color: #d0d0d0;
  color: #4f5a66;
}

.tips_mask .main .btn_group .btn.go {
  background-image: linear-gradient(270deg, #1450b0 0%, #185cc4 26%, #1f7bcd 52%, #1e96d7 80%, #209cd9 100%);
  color: #fff;
}

/* tips_mask */

