/* pages/upload/upload.wxss */

page{
  height: 100%;
  background-color: #F0F0F0;
}

.not_uploaded{
  display: flex;
  flex-direction: column;
  align-items: center;
}

.search image{
  width: 181rpx;
  height: 150rpx;
  margin-top: 199rpx;
}

.txt{
  font-size: 32rpx;
  color:#666;
  margin-top: 23rpx;
}

.go_upload{
  width: 600rpx;
  height: 90rpx;
  background:linear-gradient(90deg,rgba(32,161,224,1),rgba(18,77,184,1));
  font-size: 30rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #fff;
  margin-top: 178rpx;
}

.no_upload{
  width: 600rpx;
  height: 90rpx;
  border: 1rpx solid #20A1E0;
  font-size: 30rpx;
  color: #124DB8;
  margin-top: 70rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}