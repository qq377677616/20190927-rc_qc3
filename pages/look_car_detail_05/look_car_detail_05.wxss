/* 加视频播放效果 */
.fgbox{
	width: 100%;
	height: 100%;
	left: 0;
	top: 0;
	position: absolute;
	/* background-color: red; */
	z-index: 99;
	display: flex;
	align-items: center;
	justify-content: center; 
}
.playbtn{
	width: 90rpx;
	height: 90rpx;
	/* background-color: aqua; */
	position: relative;
	display: flex;
	justify-content: center;
	/* align-items: center; */
	border-radius: 50%;
}
.isant{
	animation: playanit .5s ease-in-out;
	animation-iteration-count: 2;
}
.playbox{
	width: 90rpx;
	height: 46rpx;
	background: center no-repeat/cover;
}
.playcen{
	width: 26rpx;
	height: 34rpx;
	background: center no-repeat/cover;
	position: absolute;
	left: 50%;
	top: 50%;
	transform: translate(-50%,-50%);
	z-index: 98;
}
.nopt{
	opacity: 0;
}
.opt{
	opacity: 1;
}
.leftbtn{
	width: 72rpx;
	height: 17rpx;
	background: center no-repeat/cover;
	z-index: 200;
	position: absolute;
	left: 0;
	top: 50%;
	transform: translateY(-50%);
}
.rightbtn{
	width: 72rpx;
	height: 17rpx;
	background: center no-repeat/cover;
	z-index: 200;
	position: absolute;
	right: 0;
	top: 50%;
	transform: translateY(-50%);
}
@keyframes playanit{
	0%{
		transform: rotate(0deg)
	}
	100%{
		transform: rotate(360deg)
	}
}

