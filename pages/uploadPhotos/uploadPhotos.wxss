page{
	background-color: #EFEFEF;
}
.uploadBox{
	width:694rpx;
	height: 531rpx;
	background-color: white;
	border-radius: 14rpx;
	margin: 0 auto;
	margin-top:28rpx;  
	padding-top: 85rpx;
	box-sizing: border-box;
}
.uploadIcon{
	width: 240rpx;
	height: 224rpx;
	background-color: #EFEFEF;
	display: flex;
	justify-content: center;
	align-items: center;
	margin: 0 auto;
}
.uploadIcon>image{
	width: 104rpx;
	height: auto;
}
.uploadTxt{
	width: 100%;
	text-align: center;
	margin-top: 36rpx;
	font-size:30rpx;
	color: #333; 
}
.limit{
	width: 100%;
	height: 24rpx;
	color: #999;
	margin-top: 10rpx;
	text-align: center;
	font-size: 24rpx;
}
.submit{
	width: 600rpx;
	height: 90rpx;
	background:linear-gradient(90deg,rgba(32,161,224,1),rgba(18,77,184,1));
	border-radius: 10rpx;
	text-align: center;
	line-height: 90rpx;
	color: #EFEFEF;
	font-size:30rpx;
	margin: 0 auto;
	margin-top: 90rpx;
}