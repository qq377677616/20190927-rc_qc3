/* xw_components/Confirm/Confirm.wxss */


.container{
    position: fixed;
    z-index:150;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.5);
}
.main{
    position: absolute;
    left: 50%;
    top: 50%;
    transform:translate3d(-50%,-50%,0);
    width: 66%;
    padding:0 86rpx;
    box-sizing: border-box;
    background-color: #fff;
    border-radius:30rpx;
}