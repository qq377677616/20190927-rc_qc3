page{
	background-color: rgba(243,243,243,1);
}
.whole{
	padding: 28rpx;
	box-sizing: border-box;
}
.hander{
	width: 100%;
	background-color: #FFFFFF;
	border-radius:14rpx;
	display: flex;
	flex-direction: column;
	padding: 0 26rpx;
	box-sizing: border-box;
}
.handrow{
	width: 100%;
	height: 120rpx;
	box-sizing: border-box;
	border-bottom: 1rpx solid rgba(238,238,238,1);
	display: flex;
	align-items: center;
}
.hander>.handrow:last-child{
	border: none;
}
.shopbox{
	width:100%;
	height:200rpx;
	background:#FFFFFF;
	border-radius:14rpx;
	margin-top: 23rpx;
	display: flex;
	padding: 19rpx;
	box-sizing: border-box;
}
.lqbtn{
	width:632rpx;
	height:74rpx;
	background:#003B73;
	border-radius:37rpx;
	font-size:30rpx;
	font-family:PingFang SC;
	font-weight:bold;
	color:rgba(255,255,255,1);
	margin: 0 auto;
	margin-top: 60rpx;
	display: flex;
	justify-content: center;
	align-items: center;
}
.shopimg{
	width: 156rpx;
	height: 156rp;
	border-radius:10rpx;
	background: center no-repeat/cover;
	flex-shrink: 0;
}
.shopinfo{
	flex: 1;
	padding-left: 27rpx;
	box-sizing: border-box;
	display: flex;
	flex-direction: column;
	justify-content: center;
}
.carName{
	font-size:30rpx;
	font-family:Source Han Sans CN;
	font-weight:500;
	color:rgba(51,51,51,1);
}
.getTime{
	font-size:24rpx;
	font-family:SourceHanSansCN;
	font-weight:400;
	color:rgba(102,102,102,1);
	margin-top: 38rpx;
}
.manPick{
	flex: 1;
	padding-left: 23rpx;
	box-sizing: border-box;
}
.icon1{
	font-size: 30rpx;
	font-weight: bold;
	color: #999;
}


