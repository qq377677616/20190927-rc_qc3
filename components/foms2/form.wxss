.yuyueshijia_form_page {
    /* padding: 28rpx; */
    position: fixed;
    width: 100%;
    /* height: 205rpx; */
    left: 0;
    bottom: 0;
    z-index: 999;
    background: transparent;
    box-sizing: border-box;
    overflow: hidden;
}

/* bind_btn */
.bind_btn {
    width: 550rpx;
    height: 70rpx;
    border: 2rpx solid #20A1E0;
    border-radius: 10rpx;
    margin: 0 auto;
    font-size: 36rpx;
    color: #20A1E0;
    display: flex;
    align-items: center;
    justify-content: center;
    margin-bottom: 20rpx;
    position: relative;
}

/* bind_btn */
.bind_btn button {
    position: absolute;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
    border: 2rpx solid #20A1E0;
    border-radius: 10rpx;
    overflow: visible;
}

/* tips */
.tips {
    font-size: 30rpx;
    color: #999;
    text-align: center;
    margin-bottom: 20rpx;
}

/* tips */


/* form_area */
.form_area {
    padding: 20rpx 20rpx;
    background-color: rgba(0, 0, 0, 0.6);
    border-radius: 14rpx;
}

.form_area .group {
    display: flex;
    align-items: center;
    margin-bottom: 20rpx;
}

.form_area .group .label {
    flex: 0 0 auto;
    font-size: 26rpx;
    color: #fff;
    /* width: 120rpx; */
    margin-right: 17rpx;
    /* text-align-last:justify;
	text-align:justify; */
    /* text-justify:distribute-all-lines;  */
    /* letter-spacing: 2rpx; */
}

.form_area .group input {
    /* flex:1; */
    height: 60rpx;
    padding: 0 20rpx;
    background-color: #F5F5F5;
    border-radius: 15rpx;
    font-size: 30rpx;
    color: #333;
}

.form_area .group input::placeholder {
    color: #999;
}

.form_area .group .code_btn {
    flex: 0 0 auto;
    width: 200rpx;
    height: 60rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 30rpx;
    color: #2A558D;
    /* background: linear-gradient(to left, rgba(18, 77, 184, 1), rgba(32, 161, 224, 1)); */
    border-radius: 6rpx;
    margin-left: 20rpx;
	position: relative;
	/* border-left: 1rpx solid rgba(0,0,0,1); */
}

.form_area .group .code_btn.gray {
    background: #ccc;
}

.form_area .group .location {
    width: 100%;
    /* flex:1; */
    padding: 0 26rpx;
    height: 60rpx;
    display: flex;
    align-items: center;
    background-color: #F5F5F5;
    border-radius: 15rpx;
    position: relative;
}

.form_area .group .location .span {
    width: 0;
    flex: 1;
    font-size: 30rpx;
    color: #666;
    text-align: center;
}

.location .icon {
    flex: 0 0 auto;
    font-size: 30rpx;
    color: #666;
    position: absolute;
    right: 0;
    padding: 12rpx 26rpx;
    top: 0;
}

.form_area .submit {
    width: 715rpx;
    height: 60rpx;
    /* background: linear-gradient(to left, rgba(18, 77, 184, 1), rgba(32, 161, 224, 1)); */
    border-radius: 10rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 28rpx;
    color: #fff;
    margin: 0 auto;
	background-color: #2A558D;
	/* background: center no-repeat/cover; */
}

picker {
    display: block;
    width: 90%;
}

picker text {
    font-size: 30rpx;
    color: #666;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    display: block;
}

.triangle {
    width: 0;
    height: 0;
    border: 14rpx solid transparent;
    border-top-color: #cdcdcd;
    position: absolute;
    right: 30rpx;
    top: 50%;
    transform: translateY(-25%);
}

.close-box {
    text-align: right;
    margin-top: -12rpx
}

.close {
    width: 60rpx;
    height: 60rpx;
    display: inline-block;
}

/* header */
.header {
    box-shadow: 0rpx 6rpx 32rpx 0rpx rgba(209, 209, 209, 0.2);
    border-radius: 14rpx;
    padding: 25rpx;
    display: flex;
    background-color: #fff;
    margin-bottom: 20rpx;
}

.header .image {
    flex: 0 0 auto;
    width: 300rpx;
    height: 176rpx;
    overflow: hidden;
    position: relative;
    border-radius: 10rpx;
    margin-right: 20rpx;
    background-repeat: no-repeat !important;
    background-size: 600rpx !important;
    background-position: -58rpx 0 !important;
}

.header .image image {
    width: 100%;
    height: 100%;
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translatex(-50%) translatey(-50%);
    border-radius: 10rpx;
}

.header .info {
    width: 0;
    flex: 1;
    display: flex;
    justify-content: center;
    flex-direction: column;
}

.header .info .title {
    margin-bottom: 40rpx;
    font-size: 30rpx;
    color: #333;
    font-weight: bold;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
}

.header .info .price {
    font-size: 24rpx;
}

.header .info .price .label {
    color: #333;
    font-weight: bold;
    margin-bottom: 8rpx;
}

.header .info .price .span {
    color: #999;
}

.header .img {
    flex: 0 0 auto;
    width: 177rpx;
    height: 177rpx;
    border: 1rpx solid rgba(229, 229, 229, 1);
    background-color: #eee;
    border-radius: 10rpx;
    box-sizing: border-box;
    position: relative;
    overflow: hidden;
    margin-right: 16rpx;
}

.header .img image {
    width: 100%;
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate3d(-50%, -50%, 0);
}

.header .content {
    width: 0;
    flex: 1;
    position: relative;
}

.header .content .h2 {
    font-size: 28rpx;
    line-height: 1.4;
    color: #333;
    font-weight: bold;
    position: absolute;
    top: 0;
    width: 100%;
}

.header .content .progress-price {
    position: absolute;
    bottom: 0;
    width: 100%;
}

.header .content .progress-price .progress {
    width: 228rpx;
    height: 24rpx;
    border-radius: 12rpx;
    background-color: #FDEBE8;
    position: relative;
    overflow: hidden;
    margin-bottom: 26rpx;
}

.header .content .progress-price .progress .bar {
    position: absolute;
    border-radius: 12rpx;
    height: 100%;
    background-color: #EF3517;
}

.header .content .progress-price .progress .text {
    position: absolute;
    width: 100%;
    height: 100%;
    font-size: 18rpx;
    color: #fff;
    line-height: 24rpx;
    padding: 0 22rpx;
    box-sizing: border-box;
}

.header .content .progress-price .price {}

.header .content .progress-price .price .span {
    font-size: 36rpx;
    font-weight: bold;
    color: #FF3735;
    display: inline-block;
    margin-right: 8rpx;
}

.header .content .progress-price .price .span:first-letter {
    font-size: 24rpx;
    font-weight: normal;
}

.header .content .progress-price .price .del {
    font-size: 24rpx;
    color: #999999;
    text-decoration: line-through;
    display: inline-block;
}

.header .content .progress-price .price .del:first-letter {
    font-size: 18rpx;
    font-weight: normal;
}



